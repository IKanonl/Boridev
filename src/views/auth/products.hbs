<main class="main-prd">
    <div class="tittle-prd">
        <h2>Listado de Productos</h2>
    </div>
    
    <div class="container-items-prd" id="container-prd">
        {{#each articulos}}
        <div class="item">
            <figure>
                <img class="img-prd" src="img/{{img}}" alt="producto">
            </figure>
            <div class="info-product">
                <h2>{{nombre}}</h2>
                <p class="price">$ {{precio}}</p>
                <button class="add-to-car" data-id="{{id}}" data-nombre="{{nombre}}" data-precio="{{precio}}" data-img="{{img}}" data-valor="{{valor}}">AÃ±adir al carrito</button>
            </div>
        </div>
        {{/each}}
    </div>
</main>